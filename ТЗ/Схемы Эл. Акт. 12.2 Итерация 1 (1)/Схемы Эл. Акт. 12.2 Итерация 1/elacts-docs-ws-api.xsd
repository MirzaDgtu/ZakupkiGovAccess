<?xml version="1.0" encoding="UTF-8"?>
<!-- EIS elacts-docs-ws-api Integration Scheme, version 12.2, create date 12.05.2022 -->
<xs:schema xmlns="http://zakupki.gov.ru/eruz/ws/elacts" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://zakupki.gov.ru/eruz/ws/elacts" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:annotation>
		<xs:documentation>Запросы в ЛКП</xs:documentation>
	</xs:annotation>
	<xs:element name="lkpGetContractsListRequest">
		<xs:annotation>
			<xs:documentation>Запрос сведений о контрактах поставщика</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="regNum" type="eruzRegistryNumType">
					<xs:annotation>
						<xs:documentation>Номер реестровой записи в ЕРУЗ</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:choice>
					<xs:sequence>
						<xs:element name="fromDate" type="xs:dateTime">
							<xs:annotation>
								<xs:documentation>Начало временного интервала, в котором ведется поиск (включается)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="toDate" type="xs:dateTime">
							<xs:annotation>
								<xs:documentation>Конец временного интервала, в котором ведется поиск (не включается)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="customerInfo" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Сведения о заказчике, с которым заключен контракт</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="INN" type="innOrganizationType">
										<xs:annotation>
											<xs:documentation>ИНН</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="KPP" type="kppType">
										<xs:annotation>
											<xs:documentation>КПП</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:element name="contractRegNum" type="contractRegNumType">
						<xs:annotation>
							<xs:documentation>Номер реестровой записи контракта</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="lkpGetContractsListResponse">
		<xs:annotation>
			<xs:documentation>Ответ на запрос сведений о контрактах поставщика</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element name="errorInfo" type="errorInfoType">
					<xs:annotation>
						<xs:documentation>Результат вызова сервиса в случае ошибки</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="contractList">
					<xs:annotation>
						<xs:documentation>Список кратких сведений о размещенных контрактах</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="contractInfo" type="contractInfoType" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Краткие сведения о размещенном контракте</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="lkpGetParticipantInfoRequest">
		<xs:annotation>
			<xs:documentation>Запрос сведений о поставщике и его подписантах</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="regNum" type="eruzRegistryNumType">
					<xs:annotation>
						<xs:documentation>Номер реестровой записи в ЕРУЗ</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="lkpGetParticipantInfoResponse">
		<xs:annotation>
			<xs:documentation>Ответ на запрос сведений о поставщике и его подписантах</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element name="errorInfo" type="errorInfoType">
					<xs:annotation>
						<xs:documentation>Результат вызова сервиса в случае ошибки</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="participantInfo">
					<xs:annotation>
						<xs:documentation>Сведения о поставщике и его подписантах</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="supplierID" type="wfParticipantID">
								<xs:annotation>
									<xs:documentation>Идентификатор участника документооборота</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="regNum" type="eruzRegistryNumType">
								<xs:annotation>
									<xs:documentation>Номер реестровой записи в ЕРУЗ</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="cabinetGUID" type="guidType">
								<xs:annotation>
									<xs:documentation>GUID кабинета организации</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="fullName" type="text1000Type">
								<xs:annotation>
									<xs:documentation>Полное наименование организации</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="signersInfo">
								<xs:annotation>
									<xs:documentation>Информация о подписантах</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="signerInfo" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Информация о подписанте</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="userId" type="xs:int">
														<xs:annotation>
															<xs:documentation>Идентификатор пользователя из ЕСИА</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="commonInfo">
														<xs:annotation>
															<xs:documentation>Общая информация о записи пользователя</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="regDate" type="xs:date">
																	<xs:annotation>
																		<xs:documentation>Дата регистрации пользователя в ЕРУЗ</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="modificationDate" type="xs:dateTime" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Дата/время последнего изменения</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="status" type="userStatusType">
																	<xs:annotation>
																		<xs:documentation>Статус пользователя:
A - Активный;
B - Заблокирован.</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="nameInfo" type="personType">
														<xs:annotation>
															<xs:documentation>ФИО подписанта</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="authoritysInfo">
														<xs:annotation>
															<xs:documentation>Информация о полномочиях</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="authorityInfo" maxOccurs="unbounded">
																	<xs:annotation>
																		<xs:documentation>Информация о полномочие</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="authorityArea" type="lkpAuthorityAreaType">
																				<xs:annotation>
																					<xs:documentation>Область полномочий</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="authorityFoundation" type="text255Type">
																				<xs:annotation>
																					<xs:documentation>Основания полномочий</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="signerType">
														<xs:annotation>
															<xs:documentation>Тип подписанта</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:choice>
																<xs:element name="individualPersonInfo">
																	<xs:annotation>
																		<xs:documentation>Физическое лицо</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="INN" type="innIndividualType">
																				<xs:annotation>
																					<xs:documentation>ИНН ФЛ</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="individualEntrepreneurInfo">
																	<xs:annotation>
																		<xs:documentation>Индивидуальный предприниматель</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="INN" type="innIndividualType">
																				<xs:annotation>
																					<xs:documentation>ИНН</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="legalEntityInfo">
																	<xs:annotation>
																		<xs:documentation>Представитель юридического лица</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="fullName" type="text1000Type">
																				<xs:annotation>
																					<xs:documentation>Полное наименование организации</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="INN" type="innOrganizationType">
																				<xs:annotation>
																					<xs:documentation>ИНН юридического лица</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="position" type="positionType" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation>Должность</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:choice>
														</xs:complexType>
													</xs:element>
													<xs:element name="isActual" type="xs:boolean">
														<xs:annotation>
															<xs:documentation>Актуальность связи пользователя с реестровой записью ЕРУЗ</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="lkpGetObjectListRequest">
		<xs:annotation>
			<xs:documentation>Запрос сведений о частично подписанных / подписанных документах электронного актирования</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="regNum" type="eruzRegistryNumType">
					<xs:annotation>
						<xs:documentation>Номер реестровой записи в ЕРУЗ</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="documentKind" type="lkpDocumentKindElActType" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Вид документа</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:choice>
					<xs:sequence>
						<xs:element name="fromDate" type="xs:dateTime">
							<xs:annotation>
								<xs:documentation>Начало временного интервала, в котором ведется поиск (включается)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="toDate" type="xs:dateTime">
							<xs:annotation>
								<xs:documentation>Конец временного интервала, в котором ведется поиск (не включается)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="customerInfo" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Сведения о заказчике</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="INN" type="innOrganizationType">
										<xs:annotation>
											<xs:documentation>ИНН</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="KPP" type="kppType">
										<xs:annotation>
											<xs:documentation>КПП</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:element name="externalId" type="externalIdType">
						<xs:annotation>
							<xs:documentation>Внешний идентификатор УПД/УКД (титул продавца)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="objectId" type="guidType">
						<xs:annotation>
							<xs:documentation>Идентификатор объекта, присвоенный в ЕИС</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="contractRegNum" type="contractRegNumType">
						<xs:annotation>
							<xs:documentation>Номер реестровой записи контракта</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="lkpGetObjectListResponse">
		<xs:annotation>
			<xs:documentation>Ответ на запрос сведений о частично подписанных / подписанных документах электронного актирования</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element name="errorInfo" type="errorInfoType">
					<xs:annotation>
						<xs:documentation>Результат вызова сервиса в случае ошибки</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="objectList">
					<xs:annotation>
						<xs:documentation>Список кратких сведений о подписанных документах электронного актирования</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="objectInfo" type="lkpObjectInfoType" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Краткие сведения о подписанном документе электронного актирования</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="lkpGetObjectInfoRequest">
		<xs:annotation>
			<xs:documentation>Запрос сведений о частично подписанном / подписанном документе электронного актирования</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="regNum" type="eruzRegistryNumType">
					<xs:annotation>
						<xs:documentation>Номер реестровой записи в ЕРУЗ</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="documentUid" type="guidType">
					<xs:annotation>
						<xs:documentation>Идентификатор документа</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="documentKind" type="text50Type">
					<xs:annotation>
						<xs:documentation>Вид документа</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="lkpGetObjectInfoResponse">
		<xs:annotation>
			<xs:documentation>Ответ на запрос сведений о частично подписанном / подписанном документе электронного актирования</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element name="errorInfo" type="errorInfoType">
					<xs:annotation>
						<xs:documentation>Результат вызова сервиса в случае ошибки</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:sequence>
					<xs:element name="objectInfo" type="lkpObjectInfoType">
						<xs:annotation>
							<xs:documentation>Cведения о документе</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:any processContents="skip">
						<xs:annotation>
							<xs:documentation>Блок содержит XML-документ, сформированный по схеме DP_PAKET_EIS_01_00</xs:documentation>
						</xs:annotation>
					</xs:any>
				</xs:sequence>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:annotation>
		<xs:documentation>Запросы в ЛКЗ</xs:documentation>
	</xs:annotation>
	<xs:element name="lkzGetSignersInfoRequest">
		<xs:annotation>
			<xs:documentation>Запрос сведений о подписантах организации</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="sender" type="organizationRef">
					<xs:annotation>
						<xs:documentation>Отправитель</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="lkzGetSignersInfoResponse">
		<xs:annotation>
			<xs:documentation>Ответ на запрос сведений о подписантах организации</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element name="errorInfo" type="errorInfoType">
					<xs:annotation>
						<xs:documentation>Результат вызова сервиса в случае ошибки</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="organizationSignersInfo">
					<xs:annotation>
						<xs:documentation>Сведения об организации и ее подписантах</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="organizationInfo" type="organizationRef">
								<xs:annotation>
									<xs:documentation>Сведения об организации</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="signersInfo" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Информация о подписантах</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="signerInfo" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Информация о подписанте</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="login" type="text100Type">
														<xs:annotation>
															<xs:documentation>Логин</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="nameInfo" type="personType">
														<xs:annotation>
															<xs:documentation>ФИО подписанта</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="userOrganizationRole" type="organizationRoleType">
														<xs:annotation>
															<xs:documentation>Полномочие организации, с которым связан пользователь организации:
CU - Заказчик;
OA - Организация, осуществляющая полномочия заказчика на осуществление закупок на основании договора (соглашения) в соответствии с частью 6 статьи 15 Закона № 44-ФЗ;
CS - Заказчик, осуществляющий закупки в соответствии с частью 5 статьи 15 Федерального закона № 44-ФЗ;
CN - Заказчик, осуществляющий закупки в соответствии с частью 4.1 статьи 15 Федерального закона № 44-ФЗ.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="status">
														<xs:annotation>
															<xs:documentation>Статус</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="code" type="lkzAuthorityAreaCodeType">
																	<xs:annotation>
																		<xs:documentation>Код статуса:
3 - Уполномоченный сотрудник иной организации;
4 - Уполномоченное физическое лицо / Уполномоченный индивидуальный предприниматель;
5 - Уполномоченный сотрудник организации заказчика;
6 - Уполномоченный сотрудник организации, осуществляющей полномочия заказчика на основании соглашения</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="name" type="text100Type">
																	<xs:annotation>
																		<xs:documentation>Наименование статуса</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
